{
    "name": "svewap/ws-scss",
    "description": "SASS compiler for TYPO3",
    "type": "typo3-cms-extension",
    "homepage": "http://wappler.systems",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "scss",
        "sass"
    ],
    "require": {
        "typo3/cms-core": "^10.4 || ^11",
        "php": "^7.4",
        "scssphp/scssphp": "^1",
        "css-crush/css-crush": "^3"
    },
    "replace": {
        "typo3-ter/ws-scss": "self.version"
    },
    "autoload": {
        "psr-4": {
            "WapplerSystems\\WsScss\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ws_scss"
        }
    },
    "authors": [
        {
            "name": "Sven Wappler",
            "role": "Developer",
            "homepage": "https://wappler.systems"
        }
    ]
}
