{
    "name": "wapplersystems/ws-scss",
    "description": "SASS compiler for TYPO3",
    "type": "typo3-cms-extension",
    "homepage": "https://wappler.systems",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "scss",
        "sass"
    ],
    "require": {
        "typo3/cms-core": "^12",
        "php": "^8.0 || ^8.1 || ^8.2",
        "scssphp/scssphp": "^1"
    },
    "replace": {
    },
    "autoload": {
        "psr-4": {
            "WapplerSystems\\WsScss\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ws_scss"
        }
    },
    "authors": [
        {
            "name": "Sven Wappler",
            "role": "Developer",
            "homepage": "https://wappler.systems"
        }
    ]
}
