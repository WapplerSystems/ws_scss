{
    "name": "svewap/ws-scss",
    "description": "SASS compiler for TYPO3",
    "type": "typo3-cms-extension",
    "homepage": "http://wappler.systems",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "scss",
        "sass"
    ],
    "authors": [
        {
            "name": "Sven Wappler",
            "role": "Developer",
            "homepage": "https://wappler.systems"
        }
    ],
    "require": {
        "typo3/cms-core": "^11",
        "php": "^7.4 || ^8.0",
        "scssphp/scssphp": "^1"
    },
    "replace": {
    },
    "autoload": {
        "psr-4": {
            "WapplerSystems\\WsScss\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ws_scss"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
